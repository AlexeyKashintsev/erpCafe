Select *
From wh_items t1
 Where (:franchazi_id = t1.franchazi_id or t1.franchazi_id is null) and 
 (:item_type = t1.item_type or :item_type is null or :item_type = 0) and
 (:item_id = t1.wh_items_id or :item_id is null)
and (:show_type is null or :show_type = 0
 or :show_type = 2 and t1.franchazi_id is not null
 or :show_type = 1 and t1.franchazi_id is not null)